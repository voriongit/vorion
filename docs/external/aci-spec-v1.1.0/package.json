{
  "name": "@agentanchor/aci-spec",
  "version": "1.0.0",
  "description": "Agent Classification Identifier (ACI) - The certification standard for AI agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./security": {
      "require": "./dist/security/index.js",
      "import": "./dist/security/index.mjs",
      "types": "./dist/security/index.d.ts"
    }
  },
  "files": [
    "dist",
    "specs",
    "docs",
    "vocab",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/security/index.ts --format cjs,esm --dts --clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "ai-agents",
    "agent-identity",
    "capability",
    "certification",
    "trust",
    "governance",
    "did",
    "openid",
    "oauth",
    "security",
    "owasp",
    "llm",
    "multi-agent"
  ],
  "author": "AgentAnchor (A3I) <spec@agentanchor.io>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voriongit/aci-spec.git"
  },
  "bugs": {
    "url": "https://github.com/voriongit/aci-spec/issues"
  },
  "homepage": "https://aci.agentanchor.io",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
