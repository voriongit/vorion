{
  "name": "@agentanchor/sdk",
  "version": "1.0.0",
  "description": "Official SDK for the AgentAnchor AI governance platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts src/react.tsx --format cjs,esm --dts --external react",
    "dev": "tsup src/index.ts src/react.tsx --format cjs,esm --dts --watch --external react",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "agentanchor",
    "ai",
    "governance",
    "trust",
    "agents",
    "sdk",
    "certification"
  ],
  "author": "AgentAnchor <dev@agentanchorai.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chunkstar/agentanchor-sdk.git"
  },
  "homepage": "https://agentanchorai.com",
  "bugs": {
    "url": "https://github.com/chunkstar/agentanchor-sdk/issues"
  },
  "dependencies": {
    "@basis-protocol/core": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
