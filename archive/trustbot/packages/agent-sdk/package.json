{
  "name": "@aurais/agent-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for connecting AI agents to Aurais Mission Control",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "aurais",
    "ai-agent",
    "mission-control",
    "websocket",
    "sdk"
  ],
  "author": "BanquetAI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BanquetAI/aurais.git",
    "directory": "packages/agent-sdk"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
