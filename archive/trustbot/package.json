{
  "name": "aurais",
  "version": "2.0.0",
  "description": "Aurais - AI Agent Governance Platform powered by Vorion BASIS",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "game": "tsx src/game/index.ts",
    "api": "tsx src/api/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "build:strict": "tsc",
    "build:prod": "npx esbuild src/api/index.ts --bundle --platform=node --target=node20 --outfile=dist/api/index.js --format=esm --packages=external",
    "start": "node dist/index.js",
    "web": "cd web && npm run dev",
    "web:build": "cd web && npm run build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "ai-agents",
    "governance",
    "trust-scoring",
    "aurais",
    "vorion",
    "basis"
  ],
  "author": "Vorion",
  "license": "MIT",
  "dependencies": {
    "@vorionsys/atsf-core": "file:./packages/atsf-core",
    "@hono/node-server": "^1.19.9",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.3",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@supabase/supabase-js": "^2.89.0",
    "date-fns": "^3.0.0",
    "dotenv": "^17.2.3",
    "eventemitter3": "^5.0.1",
    "google-auth-library": "^10.5.0",
    "hono": "^4.11.4",
    "pino": "^10.1.0",
    "prom-client": "^15.1.3",
    "uuid": "^9.0.0",
    "ws": "^8.18.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pino": "^7.0.4",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "esbuild": "^0.27.2",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  }
}
