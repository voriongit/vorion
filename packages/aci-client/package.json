{
  "name": "@vorion/aci-client",
  "version": "1.0.0",
  "description": "ACI (Agent Classification Identifier) Client SDK - TypeScript client for Phase 6 Trust Engine",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "aci",
    "agent",
    "trust",
    "governance",
    "phase6",
    "vorion",
    "ai-governance"
  ],
  "author": "Vorion",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/voriongit/vorion.git",
    "directory": "packages/aci-client"
  },
  "homepage": "https://github.com/voriongit/vorion/tree/main/packages/aci-client#readme"
}
