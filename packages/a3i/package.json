{
  "name": "@vorion/a3i",
  "version": "0.1.0",
  "description": "A3I - Agent Anchor AI Trust Engine",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./trust": {
      "types": "./dist/trust/index.d.ts",
      "import": "./dist/trust/index.js"
    },
    "./banding": {
      "types": "./dist/banding/index.d.ts",
      "import": "./dist/banding/index.js"
    },
    "./authorization": {
      "types": "./dist/authorization/index.d.ts",
      "import": "./dist/authorization/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js"
    },
    "./execution": {
      "types": "./dist/execution/index.d.ts",
      "import": "./dist/execution/index.js"
    },
    "./orchestrator": {
      "types": "./dist/orchestrator/index.d.ts",
      "import": "./dist/orchestrator/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@vorion/contracts": "*",
    "hono": "^4.6.0",
    "uuid": "^11.0.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
