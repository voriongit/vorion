{
  "name": "@vorion/agent-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for connecting AI agents to Aurais Mission Control",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "aurais",
    "vorion",
    "ai-agent",
    "mission-control",
    "websocket",
    "sdk",
    "governance"
  ],
  "author": "Vorion",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/voriongit/vorion.git",
    "directory": "packages/agent-sdk"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@fastify/jwt": "10.0.0",
    "drizzle-kit": "0.31.8",
    "eventemitter3": "^5.0.1",
    "jsonpath-plus": "10.3.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/ws": "^8.5.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
