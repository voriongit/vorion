{
  "name": "@vorion/proof-plane",
  "version": "0.1.0",
  "description": "Vorion Proof Plane - Immutable audit trail for AI agent operations",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.js"
    },
    "./proof-plane": {
      "types": "./dist/proof-plane/index.d.ts",
      "import": "./dist/proof-plane/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@vorion/contracts": "*",
    "uuid": "^11.0.4"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
